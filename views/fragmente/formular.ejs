<form id="form_inreg"  method="post" class="date"  action="<%= locals.action %>"  enctype="multipart/form-data">
    <p>
        <label>
            Username: <input type="text" name="username" id="inp-username" required value="prof<%= Math.floor(Math.random()*100000) %>" required pattern="[A-Za-z0-9]{5-7}">
        </label>
    </p>
    <p>
        <label>
            Nume: <input type="text" name="nume" id="inp-nume" required value="Gogulescu" required pattern="[A-Z][a-z]+" >
        </label>
    </p>
    <p>
        <label>
            Prenume: <input type="text" name="prenume" id="inp-prenume" required value="Gogu-Ionel" required pattern="[A-Z][a-z]+-[A-Z][a-z]+" >
        </label>
    </p>
    <p>
        <label>
            Data nastere: <input type="date" name="data_nastere" id="inp-dataN" value="">
        </label>
    </p>
    <p>
        <label>
            Ocupatie: <input type="text" name="ocupatie" id="inp-ocupatie" value=""  required pattern="[a-z]+">
        </label>
    </p>
    <p>
        <label>
            Parola: <input id="parl" type="password" required name="parola" value="" required pattern="[A-Z]+[a-z]+[0-9]+{6,}">
            <i class="bi bi-eye"></i>
        </label>
    </p>
    <p id="p-reintrod">
        <label >
            Reintroduceti parola: <input id="rparl" required type="password" name="rparola" value="">
        </label>
    </p>
    <p>
        <label>
            Email: <input type="text" name="email" id="inp-email" required value="tehniciweb43@gmail.com"  size="50">
        </label>
    </p>
    <p>
        <label>
            Culoare text chat: <select name="culoare_chat" id="sel-culoare"><option value="red" selected>rosu</option><option value="green">verde</option><option value="blue">albastru</option></select>
        </label>
    </p>
    <p>
        <label>
            Poza: <input type="file" name="poza">
        </label>			
    </p>
    <p>		
        <input type="submit" value="Trimite">
        <input type="reset" value="Reseteaza">
    </p>
</form>